{
  "name": "mcp-agentify",
  "displayName": "MCP Agentify Enhanced",
  "description": "Generate Model Context Protocol servers from your existing APIs with AI assistance, enterprise-grade features, and comprehensive debugging tools",
  "version": "2.0.0",
  "publisher": "SaurabhKohli",
  "author": "Saurabh Kohli <me.saurabhkohli@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/me-saurabhkohli/agentify.git"
  },
  "bugs": {
    "url": "https://github.com/me-saurabhkohli/agentify/issues"
  },
  "homepage": "https://github.com/me-saurabhkohli/agentify#readme",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Extension Packs"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "api",
    "rest",
    "code-generation",
    "typescript",
    "enterprise"
  ],
  "activationEvents": [
    "onCommand:mcp-agentify.analyze",
    "onCommand:mcp-agentify.generate",
    "onCommand:mcp-agentify.interactive",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:java",
    "onLanguage:go",
    "workspaceContains:package.json",
    "workspaceContains:requirements.txt",
    "workspaceContains:pom.xml",
    "workspaceContains:go.mod"
  ],
  "main": "./out/extension.js",
  "bin": {
    "mcp-debug-cli": "./bin/mcp-debug-cli.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "mcp-agentify.analyze",
        "title": "Analyze Project",
        "category": "MCP Agentify",
        "icon": "$(search)"
      },
      {
        "command": "mcp-agentify.generate",
        "title": "Generate MCP Server",
        "category": "MCP Agentify",
        "icon": "$(rocket)"
      },
      {
        "command": "mcp-agentify.interactive",
        "title": "Interactive Setup",
        "category": "MCP Agentify",
        "icon": "$(wand)"
      },
      {
        "command": "mcp-agentify.quickSetup",
        "title": "Quick Setup",
        "category": "MCP Agentify",
        "icon": "$(zap)"
      },
      {
        "command": "mcp-agentify.openWebview",
        "title": "Open Agentify Panel",
        "category": "MCP Agentify",
        "icon": "$(browser)"
      },
      {
        "command": "mcp-agentify.viewEndpoints",
        "title": "View Detected Endpoints",
        "category": "MCP Agentify",
        "icon": "$(list-unordered)"
      },
      {
        "command": "mcp-agentify.installCLI",
        "title": "Install/Update CLI",
        "category": "MCP Agentify",
        "icon": "$(cloud-download)"
      },
      {
        "command": "mcp-agentify.showDocumentation",
        "title": "Show Documentation",
        "category": "MCP Agentify",
        "icon": "$(book)"
      },
      {
        "command": "mcp-agentify.openSettings",
        "title": "Open Settings",
        "category": "MCP Agentify",
        "icon": "$(gear)"
      },
      {
        "command": "mcp-agentify.debug.start",
        "title": "Start Debugging MCP Server",
        "category": "MCP Agentify Debug",
        "icon": "$(debug-start)"
      },
      {
        "command": "mcp-agentify.debug.attach",
        "title": "Attach to MCP Server",
        "category": "MCP Agentify Debug",
        "icon": "$(debug-console)"
      },
      {
        "command": "mcp-agentify.debug.listTools",
        "title": "List MCP Tools",
        "category": "MCP Agentify Debug",
        "icon": "$(list-unordered)"
      },
      {
        "command": "mcp-agentify.debug.inspectContext",
        "title": "Inspect MCP Context",
        "category": "MCP Agentify Debug",
        "icon": "$(inspect)"
      },
      {
        "command": "mcp-agentify.mcpServer.start",
        "title": "Start MCP Debug Server",
        "category": "MCP Agentify",
        "icon": "$(play)"
      },
      {
        "command": "mcp-agentify.mcpServer.stop",
        "title": "Stop MCP Debug Server",
        "category": "MCP Agentify",
        "icon": "$(stop)"
      },
      {
        "command": "mcp-agentify.mcpServer.status",
        "title": "Show MCP Server Status",
        "category": "MCP Agentify",
        "icon": "$(pulse)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "mcp-agentify.analyze",
          "when": "explorerResourceIsFolder",
          "group": "mcp-agentify@1"
        },
        {
          "command": "mcp-agentify.generate",
          "when": "explorerResourceIsFolder",
          "group": "mcp-agentify@2"
        }
      ],
      "editor/context": [
        {
          "command": "mcp-agentify.analyze",
          "when": "resourceExtname =~ /\\.(js|ts|py|java|go)$/",
          "group": "mcp-agentify@1"
        }
      ],
      "commandPalette": [
        {
          "command": "mcp-agentify.analyze",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "mcp-agentify.generate",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "mcp-agentify.interactive",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "mcp-agentify.quickSetup",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mcp-agentify",
          "title": "MCP Agentify",
          "icon": "$(rocket)"
        }
      ]
    },
    "views": {
      "mcp-agentify": [
        {
          "id": "mcp-agentify.projectOverview",
          "name": "Project Overview",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "mcp-agentify.detectedEndpoints",
          "name": "Detected Endpoints",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "mcp-agentify.generatedServers",
          "name": "Generated Servers",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "mcp-agentify.plugins",
          "name": "Plugins",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "mcp-agentify.projectOverview",
        "contents": "Welcome to MCP Agentify! ðŸš€\n\nAnalyze your project to detect API endpoints and generate MCP servers.\n\n[Analyze Project](command:mcp-agentify.analyze)\n[Interactive Setup](command:mcp-agentify.interactive)\n[Quick Setup](command:mcp-agentify.quickSetup)\n\nNeed the CLI? [Install CLI](command:mcp-agentify.installCLI)"
      }
    ],
    "configuration": {
      "title": "MCP Agentify",
      "properties": {
        "mcp-agentify.cliPath": {
          "type": "string",
          "default": "npx mcp-agentify",
          "description": "Path to MCP Agentify CLI command"
        },
        "mcp-agentify.autoAnalyze": {
          "type": "boolean",
          "default": true,
          "description": "Automatically analyze project when workspace is opened"
        },
        "mcp-agentify.outputDirectory": {
          "type": "string",
          "default": "./mcp-server",
          "description": "Default output directory for generated MCP servers"
        },
        "mcp-agentify.preferredFormat": {
          "type": "string",
          "enum": [
            "typescript",
            "javascript"
          ],
          "default": "typescript",
          "description": "Preferred output format for generated code"
        },
        "mcp-agentify.includeTests": {
          "type": "boolean",
          "default": true,
          "description": "Include test files in generated projects"
        },
        "mcp-agentify.includeDocumentation": {
          "type": "boolean",
          "default": true,
          "description": "Include documentation in generated projects"
        },
        "mcp-agentify.enableSecurity": {
          "type": "boolean",
          "default": false,
          "description": "Enable enterprise security features"
        },
        "mcp-agentify.enablePlugins": {
          "type": "boolean",
          "default": true,
          "description": "Enable plugin system"
        },
        "mcp-agentify.showNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show success/error notifications"
        },
        "mcp-agentify.debugMode": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug mode for troubleshooting"
        },
        "mcp-agentify.autoStartMCPServer": {
          "type": "boolean",
          "default": true,
          "description": "Automatically start MCP Debug Server when extension activates"
        },
        "mcp-agentify.mcpServerPort": {
          "type": "number",
          "default": 8890,
          "description": "Port for MCP Debug Server"
        }
      }
    },
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/mcp-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/mcp-snippets.json"
      }
    ],
    "taskDefinitions": [
      {
        "type": "mcp-agentify",
        "required": [
          "command"
        ],
        "properties": {
          "command": {
            "type": "string",
            "description": "The MCP Agentify command to run"
          },
          "args": {
            "type": "array",
            "description": "Command arguments"
          }
        }
      }
    ],
    "debuggers": [
      {
        "type": "mcp-agentify",
        "label": "MCP Agentify Debug",
        "program": "./out/debugAdapter.js",
        "runtime": "node",
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "Absolute path to the MCP server entry point",
                "default": "${workspaceFolder}/mcp-server/dist/index.js"
              },
              "args": {
                "type": "array",
                "description": "Command line arguments passed to the program",
                "items": {
                  "type": "string"
                },
                "default": []
              },
              "cwd": {
                "type": "string",
                "description": "Absolute path to the working directory of the program being debugged",
                "default": "${workspaceFolder}/mcp-server"
              },
              "env": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                },
                "description": "Environment variables passed to the program",
                "default": {}
              },
              "port": {
                "type": "number",
                "description": "Debug port",
                "default": 9229
              },
              "host": {
                "type": "string",
                "description": "Debug host",
                "default": "localhost"
              },
              "trace": {
                "type": "boolean",
                "description": "Enable logging of the Debug Adapter Protocol",
                "default": false
              },
              "console": {
                "enum": [
                  "internalConsole",
                  "integratedTerminal",
                  "externalTerminal"
                ],
                "description": "Where to launch the debug target",
                "default": "integratedTerminal"
              }
            }
          },
          "attach": {
            "required": [
              "port"
            ],
            "properties": {
              "port": {
                "type": "number",
                "description": "Debug port to attach to",
                "default": 9229
              },
              "host": {
                "type": "string",
                "description": "Debug host to attach to",
                "default": "localhost"
              },
              "trace": {
                "type": "boolean",
                "description": "Enable logging of the Debug Adapter Protocol",
                "default": false
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "mcp-agentify",
            "request": "launch",
            "name": "Debug MCP Server",
            "program": "${workspaceFolder}/mcp-server/dist/index.js",
            "cwd": "${workspaceFolder}/mcp-server",
            "console": "integratedTerminal",
            "internalConsoleOptions": "openOnSessionStart"
          }
        ],
        "configurationSnippets": [
          {
            "label": "MCP Agentify: Launch",
            "description": "A new configuration for launching an MCP server",
            "body": {
              "type": "mcp-agentify",
              "request": "launch",
              "name": "Debug MCP Server",
              "program": "^\"\\${workspaceFolder}/mcp-server/dist/index.js\"",
              "cwd": "^\"\\${workspaceFolder}/mcp-server\"",
              "console": "integratedTerminal"
            }
          },
          {
            "label": "MCP Agentify: Attach",
            "description": "A new configuration for attaching to a running MCP server",
            "body": {
              "type": "mcp-agentify",
              "request": "attach",
              "name": "Attach to MCP Server",
              "port": 9229,
              "host": "localhost"
            }
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test:unit": "npx mocha out/test/suite/*.test.js --timeout 10000",
    "test:integration": "npx mocha out/test/suite/integration.test.js --timeout 15000",
    "test:debug": "npx mocha out/test/suite/debugSession.test.js --timeout 10000",
    "test:config": "npx mocha out/test/suite/debugConfigurationProvider.test.js --timeout 10000",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/sinon": "^10.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^3.6.2",
    "eslint": "^8.28.0",
    "glob": "^11.0.3",
    "mocha": "^10.8.2",
    "sinon": "^15.2.0",
    "typescript": "^4.9.4",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@vscode/debugadapter": "^1.65.0",
    "@vscode/debugprotocol": "^1.65.0",
    "await-notify": "^1.0.1",
    "axios": "^1.6.0",
    "express": "^5.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  }
}
